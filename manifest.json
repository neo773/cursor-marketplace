{
  "manifest_version": 3,
  "name": "Cursor Marketplace Installer",
  "version": "1.0.1",
  "description": "Adds a Cursor install button to VS Code Extensions Marketplace",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://marketplace.visualstudio.com/items*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
